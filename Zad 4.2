#include <iostream>

using namespace std;

int main(){
    int x;
    cout << "Jaka jest dlugosc tablicy?: ";
    cin>>x;
 
    int arr[x];
 
    for(int i=0; i<x; i++){
        cout << "Jaka liczba znajduje siÄ™ na pozycji: " << i + 1 << endl;
        cin>>arr[i];
        cout << endl;
    }
    cout << "Twoja tablica to: ";
	for (int z = 0; z < x; z++){
		cout << arr[z];
	}
    cout << endl;
    
    int res = 0;
    int max = 0;
	
	for (int i = 0; i < x; i++){
		int count = 0;
		for (int j = 0; j < x; j++) {
			if (arr[i] == arr[j])
				count++;
		}

		if (count > max){
			max = count;
			res = arr[i];
			
		}
	}
	cout << "Najczesciej wystepujacym elementem jest: " << res;
}
